	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="userBean" class="ga.fumyparli.diexam01.UserBean"></bean>
	
	<bean id="e" class="ga.fumyparli.diexam01.Engine"></bean>
	<bean id="c" class="ga.fumyparli.diexam01.Car">
